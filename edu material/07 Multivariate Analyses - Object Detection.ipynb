    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"#Transfer the numbers into the 5 different classes \\n\",\n",
    "    \"labels = { 1:'car', 2:'truck', 3:'person', 4:'bicycle', 5:'traffic light'}\"\n",
    "   ]\n",
    "  },\n",
