    "    \"from sklearn.utils import shuffle\\n\",\n",
    "    \"\\n\",\n",
    "    \"#Load the labels which are vectors with a reference to the image\\n\",\n",
    "    \"df_target = pd.read_csv('labels_train.csv')\\n\",\n",
    "    \"df_target = shuffle(df_target, random_state=34)\\n\",\n",
    "    \"df_target.head()\"\n",
